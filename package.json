    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.4.0",
    "lighthouse": "^3.2.1",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.8.4"
  },
  "keywords": [
    "gatsby",
