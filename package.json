    "babel-plugin-emotion": "^10.0.6",
    "emotion-theming": "^10.0.6",
    "fs": "0.0.1-security",
    "gatsby": "^2.2.9",
    "gatsby-image": "^2.0.26",
    "gatsby-paginate": "^1.0.17",
    "gatsby-plugin-baidu-analytics": "^1.1.16",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-google-analytics": "^2.0.9",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-plugin-sitemap": "^2.0.4",
    "gatsby-plugin-stylus": "^2.0.6",
